# Decrease brightness (Arch: XF86, FreeBSD: F5)
"brightnessctl set 10%-"
    XF86MonBrightnessDown

"backlight decr 10"
    F5

# Increase brightness
"brightnessctl set +10%"
    XF86MonBrightnessUp

"backlight incr 10"
    F6

# Simple ALSA Volume Controls
"~/repos/dotfiles/scripts/volume-up.sh"
    XF86AudioRaiseVolume

"~/repos/dotfiles/scripts/volume-down.sh"
    XF86AudioLowerVolume

"~/repos/dotfiles/scripts/volume-mute.sh"
    XF86AudioMute
