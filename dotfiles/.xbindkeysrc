# Decrease brightness
"~/repos/dotfiles/scripts/brightness-down.sh"
    XF86MonBrightnessDown

# Increase brightness
"~/repos/dotfiles/scripts/brightness-up.sh"
    XF86MonBrightnessUp


# Simple ALSA Volume Controls
"~/repos/dotfiles/scripts/volume-up.sh"
    XF86AudioRaiseVolume

"~/repos/dotfiles/scripts/volume-down.sh"
    XF86AudioLowerVolume

"~/repos/dotfiles/scripts/volume-mute.sh"
    XF86AudioMute